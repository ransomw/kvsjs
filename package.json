{
  "name": "pd-store",
  "version": "0.0.0",
  "type": "app",
  "dependencies": {
    "immutable": "^3.8.2",
    "ramda": "^0.25.0",
    "daggy": "^1.2.0",
    "monet": "^0.8.10",
    "parsimmon": "^1.6.2"
  },
  "devDependencies": {
    "docco": "^0.7.0",
    "jshint": "^2.9.5",
    "tape": "^4.8.0",
    "tap-spec": "^4.1.1"
  },
  "scripts": {
    "doc": "docco -o doc/src src/*.js; docco -o doc/src/fn do src/fn/*.js",
    "interpreter": "node bin/interpreter",
    "lint": "jshint src/*.js test/*.js bin/*.js",
    "test": "tape test/*.test.js | tap-spec"
  },
  "engines": {
    "node": "8.3.0"
  }
}
